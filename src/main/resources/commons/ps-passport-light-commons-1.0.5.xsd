            <xs:element name="inDocumentGroups" type="tns:InDocumentGroupListType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Группы входящих документов</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="targetScenarios" type="tns:TargetScenarioListType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Сценарии завершения</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="url" type="ct:text" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Ссылка на описание цели обращения в сети Интернет</xs:documentation>
