            <xs:element name="responsibleRGUStateStructure" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Ответственный орган власти, в том числе и за ведение услуги в реестре
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="responsibleOrganization" type="xs:boolean">
