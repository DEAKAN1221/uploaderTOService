            <xs:element name="url" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Адрес в сети интернет</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="status" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Статус услуги</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="createDate" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Дата создания</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="startPublishDate" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Дата начала оказания</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="endPublishDate" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Дата окончания оказания</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="typicalServiceTemplate" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Сведения о шаблоне типовой услуги, на основании которой создана данная
