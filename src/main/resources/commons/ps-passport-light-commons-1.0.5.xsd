            <xs:element name="responsibleOrganization" type="ct:text" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Ответственная организация</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="shortTitle" type="ct:text" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Краткое наименование</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="url" type="ct:text" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Адрес в сети Интернет</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="status" type="ct:PsPassportStatusType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Статус услуги</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="createDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата создания</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="startPublishDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата начала оказания</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="endPublishDate" type="xs:date" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Дата окончания оказания</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="administrativeLevel" type="ct:ReferenceItem" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Административный уровень</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="classificator" type="ct:ReferenceItem" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Классификатор услуг/функций</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="category" type="ct:ReferenceItem" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Категория услуги</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="typicalServiceTemplate" type="tns:TypicalServiceTemplateType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Информация о шаблоне типовой услуги, на основании которой создана данная
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="participants" type="tns:ParticipantListType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Участвующие организации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="services" type="tns:ServiceListType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Процедуры взаимодействия с заявителями/Проведения проверок</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="TypicalServiceTemplateType">
        <xs:annotation>
            <xs:documentation>Информация о шаблоне типовой услуги, на основании которой создана данная
