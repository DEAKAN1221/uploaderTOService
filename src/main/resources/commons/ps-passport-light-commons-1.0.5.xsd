        <xs:sequence>
            <xs:element name="outDocuments" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Список исходящих документов</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="PsPassportType">
        <xs:annotation>
