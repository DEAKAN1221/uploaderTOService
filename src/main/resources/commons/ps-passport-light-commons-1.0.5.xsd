                </xs:annotation>
            </xs:element>
            <xs:element name="title" type="ct:text" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Наименование</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="groundForAction" type="ct:text" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Основание</xs:documentation>
                </xs:annotation>
