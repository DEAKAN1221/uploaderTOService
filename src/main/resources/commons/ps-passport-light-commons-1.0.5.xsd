    <xs:complexType name="ServiceFilterType">
        <xs:annotation>
            <xs:documentation>Фильтр по процедурам взаимодействия с заявителем</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="groundForAction" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Основание</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="result" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Результат оказания услуги</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="serviceTargets" type="tns:ServiceTargetFilterType">
                <xs:annotation>
                    <xs:documentation>Цели обращения</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ServiceTargetFilterType">
        <xs:annotation>
            <xs:documentation>Фильтр целей обращения</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="executionTime" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Срок предоставления</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="url" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Ссылка на описание цели обращения в сети Интернет</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="payments" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Оплата</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="lifeEvents" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Жизненные ситуации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="recipientCategories" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Категории получателей</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="inDocuments" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Входящие документы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="inDocumentGroups" type="tns:InDocumentGroupFilterType">
                <xs:annotation>
                    <xs:documentation>Группы входящих документов</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="targetScenarios" type="tns:TargetScenarioFilterType">
                <xs:annotation>
                    <xs:documentation>Сценарии завершения</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="InDocumentGroupFilterType">
        <xs:annotation>
            <xs:documentation>Фильтр группы входящих документов</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="title" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Заголовок группы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="inDocuments" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Список документов группы</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="TargetScenarioFilterType">
        <xs:annotation>
            <xs:documentation>Фильтр сценариев завершения</xs:documentation>
