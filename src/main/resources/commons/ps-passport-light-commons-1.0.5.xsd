            <xs:element name="participants" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Участвующие организации</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="services" type="tns:ServiceFilterType">
                <xs:annotation>
                    <xs:documentation>Процедуры взаимодействия с заявителем/Проведения проверок</xs:documentation>
                </xs:annotation>
            </xs:element>
